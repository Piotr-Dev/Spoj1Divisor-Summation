        public static int SumDividers(int number)
        {
            int dividersSum = 0;
            for (int divider = 1; divider < number; divider++)
            {
                bool isDividable = number % divider == 0;
                if (isDividable)
                {
                    dividersSum += divider;
                }              
            }
            return dividersSum;

